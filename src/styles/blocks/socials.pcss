.socials {
  display: flex;
}

.socials__list {
  display: flex;
}

.socials__item {
    max-width: 35px;
    margin-right: 20px;
}

.socials__link {
  width: 25px;
  height: 25px;
  display: block;
}

.socials__item:last-child {
  margin-right: 0;
}

.socials__icon {
  width: 100%;
  height: 100%;
  fill: #f5deb3;
}