.feedback-sec {
    position: relative;
    padding: 150px 0;
}

.feedback-sec__bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -9999;
    object-fit: cover;
}

.feedback-sec__form {
  margin-top: 195px;
}

.feedback-sec__title {
    margin-bottom: 90px;
}

@media screen and (max-width: 480px) {
    .feedback-sec {
        padding: 20px 0;
    }
}